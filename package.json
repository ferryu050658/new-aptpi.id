{
    "name": "aptpi.id",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://aptpi.id",
    "scripts": {
        "dev": "next dev -p 3000",
        "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
        "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
        "build": "next build",
        "start": "node server.js",
        "lint": "next lint",
        "typecheck": "tsc --noEmit",
        "cpanel:build": "npm install && npm run build",
        "cpanel:start": "NODE_ENV=production node server.js",
        "cpanel:setup": "npm install && npm run build && npm run cpanel:start",
        "cpanel:install": "npm ci --production",
        "cpanel:restart": "pkill -f 'node server.js' && npm run cpanel:start",
        "cpanel:logs": "tail -f app.log",
        "cpanel:deploy": "npm run cpanel:install && npm run cpanel:build && npm run cpanel:restart",
        "local:setup": "bash scripts/local-setup.sh",
        "local:check": "node scripts/check-requirements.js",
        "local:db-reset": "mysql -u root -p aptpi_local < supabase/migrations/20250606161201_gentle_meadow.sql",
        "local:db-backup": "mysqldump -u root -p aptpi_local > backup_$(date +%Y%m%d_%H%M%S).sql"
    },
